<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@android:color/white">
    <nu.pto.androidapp.base.ui.PtoTextView
        android:id="@+id/pto_text_view_set_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/set_bg"
        android:textColor="@android:color/white"
        app:font_name="@string/FONT_REGULAR"
        android:textSize="16dp"
        android:padding="10dp"/>

    <RelativeLayout
        android:id="@+id/repeat_relative_layout_workout_expandable_list_group"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <ImageView
            android:id="@+id/repeat_icon_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_repeat"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_margin="15dp"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:text="@string/string_repeat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/repeat_icon_workout_expandable_list_group"
            android:paddingLeft="10dp"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/repeat_set_value_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="15dp"
            android:layout_toLeftOf="@+id/repeat_set_unit_workout_expandable_list_group"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/repeat_set_unit_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string_time_unit"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:visibility="visible"
            android:background="#e8e8e8e8"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/weight_relative_layout_workout_expandable_list_group"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <ImageView
            android:id="@+id/weight_icon_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_weight"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_margin="15dp"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:text="@string/string_effort"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/weight_icon_workout_expandable_list_group"
            android:paddingLeft="10dp"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/weight_set_value_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="15dp"
            android:layout_toLeftOf="@+id/weight_set_unit_workout_expandable_list_group"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/weight_set_unit_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:visibility="visible"
            android:background="#e8e8e8e8"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rest_relative_layout_workout_expandable_list_group"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <ImageView
            android:id="@+id/rest_icon_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_rest"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_margin="15dp"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:text="@string/string_rest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/rest_icon_workout_expandable_list_group"
            android:paddingLeft="10dp"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/rest_set_value_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="15dp"
            android:layout_toLeftOf="@+id/rest_set_unit_workout_expandable_list_group"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/rest_set_unit_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:visibility="visible"
            android:background="#e8e8e8e8"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/distance_relative_layout_workout_expandable_list_group"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <ImageView
            android:id="@+id/distance_icon_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_distance"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_margin="15dp"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:text="@string/string_distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/distance_icon_workout_expandable_list_group"
            android:paddingLeft="10dp"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/distance_set_value_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="15dp"
            android:layout_toLeftOf="@+id/distance_set_unit_workout_expandable_list_group"
            android:layout_centerVertical="true"/>
        <nu.pto.androidapp.base.ui.PtoTextView
            android:id="@+id/distance_set_unit_workout_expandable_list_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:visibility="visible"
            android:background="#e8e8e8e8"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
</LinearLayout>